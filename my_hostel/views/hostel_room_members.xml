<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_hostel_room_members_form" model="ir.ui.view">
            <field name="name">Hostel room members form</field>
            <field name="model">hostel.room.member</field>
            <field name="arch" type="xml">
                <form string="hostel room member Information">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="email"/>
                            <field name="phone"/>
                            <field name="checkin_date"/>
                            <field name="checkout_date"/>
                            <field name="room_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
</odoo>